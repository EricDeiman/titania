module irarraytest

    from
        std use showi

    type
        arrayOfInt = integer array ;

        threeInts = record
            one : integer ;
            two : integer ;
            three : integer
        end ;

    var
        testIntArray : arrayOfInt ;
        x : integer ;
        test3Ints : threeInts

begin
    x <- testIntArray[ 3 ] ;
    testIntArray[ 5 ] <- 42 ;

    x <- test3Ints.one ;
    test3Ints.two <- 2 ;

end